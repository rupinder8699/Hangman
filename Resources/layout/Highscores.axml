        <Button
            android:id="@+id/profilebutton4"
            android:visibility="invisible"
            android:layout_height="40sp"
            android:layout_width="100sp"
            android:text="Go"/>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/highscore5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
